# Search Head member clustering
[clustering]
mode = searchhead
master_uri={{protocol}}://{{cluster_master_grp[0]}}:{{mgmtPort}}
pass4SymmKey = my_pass
multisite = true
cluster_label = my_shc_cluster1

[shclustering]
conf_deploy_fetch_url = {{protocol}}://{{cluster_master_grp[0]}}:{{mgmtPort}}
pass4SymmKey = my_pass
shcluster_label = my_shc_cluster1
disabled = false

[replication_port://{{replicationPort}}]
disabled = false
