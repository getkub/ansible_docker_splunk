#indexer clustering
[clustering]
mode = slave
master_uri={{protocol}}://{{clusterMaster[0]}}:{{mgmtPort}}
pass4SymmKey = my_pass
cluster_label = my_idx_cluster1

[replication_port://{{replicationPort}}]
disabled = false
rootCA = $SPLUNK_HOME/etc/apps/{{certApp}}/bin/auth/rootCA.pem
serverCert = $SPLUNK_HOME/etc/apps/{{certApp}}/bin/auth/device.pem
password = {{certAppPass}} 
requireClientCert = false

